<form>
	<field>
		<id>dialplan.enabled</id>
		<label>Enabled</label>
		<type>checkbox</type>
		<help>Enable this Codec</help>
	</field>
	<field>
		<id>dialplan.name</id>
		<label>Name</label>
		<type>text</type>
		<help>Name of the Codec</help>
	</field>
	<field>
		<id>dialplan.description</id>
		<label>Description</label>
		<type>text</type>
		<help>Description of this Codec (Not Parsed)</help>
	</field>
	<field>
		<id>dialplan.inboundDialPattern</id>
		<label>Inbound Dial Pattern</label>
		<type>text</type>
		<help>Pattern to match</help>
	</field>
	<field>
		<id>dialplan.outboundDialString</id>
		<label>Outbound Dial String</label>
		<type>text</type>
		<help>Pattern to match</help>
	</field>
	<field>
		<id>dialplan.contacts</id>
		<label>Contacts</label>
		<type>select_multiple</type>
		<allownew>true</allownew>
		<help>Contacts to attempt to dial when pattern matched</help>
	</field>
</form>